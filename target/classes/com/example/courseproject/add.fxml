<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="527.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.courseproject.AddController">
   <children>
      
      <Label layoutX="299.0" layoutY="55.0" prefHeight="26.0" prefWidth="302.0" text="Добавьте свою запись">
         <font>
            <Font name="Serif Regular" size="29.0" />
         </font>
      </Label>

      <Button fx:id="add" layoutX="684.0" layoutY="435.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="156.0" style="-fx-background-color: #008f24; -fx-background-radius: 20;" text="Добавить" textFill="WHITE">
         <font>
            <Font name="Serif Regular" size="18.0" />
         </font>
      </Button>
      
      <TextField fx:id="subjectField" layoutX="85.0" layoutY="189.0" prefHeight="42.0" prefWidth="336.0" promptText="Предмет">
         <font>
            <Font name="Serif Regular" size="14.0" />
         </font></TextField>
      <TextField fx:id="teacherField" layoutX="85.0" layoutY="112.0" prefHeight="42.0" prefWidth="336.0" promptText="Преподаватель">
         <font>
            <Font name="Serif Regular" size="14.0" />
         </font>
      </TextField>
      <TextArea fx:id="quoteField" layoutX="85.0" layoutY="269.0" prefHeight="200.0" prefWidth="411.0" promptText="Цитата">
         <font>
            <Font name="Serif Regular" size="14.0" />
         </font>
      </TextArea>
      <Button fx:id="backButton" layoutX="44.0" layoutY="38.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="82.0" style="-fx-background-radius: 20; -fx-background-color: #1E90FF;" text="Back" textFill="WHITE" />

   </children>
</AnchorPane>
